{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "env": {
    "mocha": true
  },
  "globals": {
    "sinon": true,
    "expect": true
  },
  "rules": {
    "no-plusplus": 0,
    "comma-dangle": 0,
    "no-continue": 0,
    "arrow-parens": 0,
    "arrow-body-style": 0,
    "import/prefer-default-export": 0,
    "no-param-reassign": 0,
    "quote-props": 0,
    "prefer-template": 0,
    "prefer-const": 0,
    "no-use-before-define": 0,
    "prefer-spread": 0,
    "space-infix-ops": 0,
    "no-multi-spaces": 0,
    "consistent-return": 0,
    "array-bracket-spacing": 0,
    "object-curly-spacing": 0,
    "radix": 0,
    "comma-spacing": 0,
    "camelcase": 0,
    "func-names": 0,
    "space-in-parens": 0,
    "prefer-arrow-callback": 0,
    "import/extensions": 0,
    "import/no-mutable-exports": 0,
    "no-path-concat": 0,
    "no-trailing-spaces": 0,
    "no-spaced-func": 0,
    "func-call-spacing": 0,
    "dot-notation": 0,
    "no-lonely-if": 0,
    "operator-assignment": 0,
    "no-multi-assign": 0,
    "spaced-comment": 0,
    "object-shorthand": 0,
    "global-require": 0,
    "class-methods-use-this": 0,
    "no-new": 0
  }
}
